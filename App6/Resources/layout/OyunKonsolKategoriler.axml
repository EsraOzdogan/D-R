<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
     <GridLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="49.5dp"
        android:id="@+id/gridLayout1"
        android:background="#ff3ac121">
        <ImageButton
            android:src="@android:drawable/ic_menu_gallery"
            android:id="@+id/ımageButton10"
            android:layout_marginRight="0.0dp"
            android:layout_marginLeft="0.0dp" />
        <TextView
            android:text="GERİ DÖN"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView1"
            android:textColor="#ffffffff" />
        <ImageButton
            android:src="@android:drawable/ic_menu_gallery"
            android:id="@+id/ımageButton20"
            android:layout_marginRight="0.0dp" />
        <ImageButton
            android:src="@android:drawable/ic_menu_gallery"
            android:id="@+id/ımageButton19" />
    </GridLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="38.5dp"
        android:id="@+id/linearLayout1" >
        <Button
            android:text="Oyun-Konsol"
            android:layout_width="187.5dp"
            android:layout_height="match_parent"
            android:id="@+id/button4"
            android:textColor="#ffeee0e0"
            android:background="#ff3ac121" />
        <Button
            android:text="Kategoriler"
            android:layout_width="196.5dp"
            android:layout_height="match_parent"
            android:id="@+id/button5"
            android:textColor="#fffcfafa"
            android:background="#ff3ac121" />
    </LinearLayout>
    <Button
        android:text="Oyun Konsollar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button6" />
    <Button
        android:text="Oyunlar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button7" />
    <Button
        android:text="Koleksiyon Ürünleri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button8" />
    <Button
        android:text="Oyun Aksesuarları"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button9" />
</LinearLayout>
